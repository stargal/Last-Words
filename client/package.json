{
  "name": "last_words",
  "version": "1.0.0",
  "description": "A simple plugin to show the last words of a player",
  "keywords": [
    "typescript",
    "multiplatform"
  ],
  "author": "stargal",
  "homepage": "https://github.com/stargal",
  "main": "dist/main.build.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=dev webpack --config ./webpack.config.js",
    "dev-watch": "cross-env NODE_ENV=dev NODE_ENV=dev webpack --config ./webpack.config.js --watch",
    "serve-watch": "cross-env NODE_ENV=dev webpack serve --config ./webpack.config.js",
    "build": "cross-env NODE_ENV=prod NODE_ENV=prod webpack --config ./webpack.config.js",
    "eslint": "npx eslint -c .eslintrc.js --ext .js,.jsx,.ts,.tsx ."
  },
  "license": "GPL-3.0",
  "dependencies": {
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "ts-loader": "^9.4.1"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "html-webpack-plugin": "^5.5.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1",
    "webpack-obfuscator": "^3.5.1",
    "webpack-shell-plugin-next": "^2.3.1"
  }
}
